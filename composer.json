{
    "name": "timino/timino",
    "type": "framework",
    "description": "Timino is a simple, lightweight and S.O.L.I.D PHP MVC framework",
    "keywords": ["timino","tim", "php","php mvc", "php simple framework", "php solid", "php lightweight mvc"],
    "homepage": "https://github.com/lotfio-lakehal/Timino",
    "license": "MIT",
    "authors": [
        {
            "name": "lotfio lakehal",
            "email": "lotfiolakehal@gmail.com",
            "role": "Developer"
        }
    ],
    
    "minimum-stability": "alpha",

    "require": {
        "php": ">=7.0",
        "twig/twig": "^2.4"
    },

    "require-dev": {
        "phpunit/phpunit":">=6.3.0",
        "timinocli/timinocli": ">=0.2.0"
    },

    "autoload":{
        "psr-4": {
            "App\\" : "App/"
        }
    },
    "autoload-dev":{
        "psr-4": {
            "Tests\\":"tests/"
        }
    }
}
